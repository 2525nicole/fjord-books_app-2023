<h1><%= t('title.index', model: User.model_name.human) %></h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to t('views.show'), user %>
    </p>
  <% end %>
</div>

<%= paginate @users %>

<%= link_to t('views.back', model: Book.model_name.human), books_path %>
